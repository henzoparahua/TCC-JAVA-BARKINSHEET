<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="mainContainer" fx:id="anchorMain" prefHeight="480.0" prefWidth="720.0"
            stylesheets="@../../Styles/driverinfo.css" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.helpfox.main.Controller.User.DriverInfo">
   <Label fx:id="name" text="s"/>
   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="12.0" AnchorPane.rightAnchor="300"
         AnchorPane.topAnchor="80">
      <children>
         <RadioButton fx:id="radioPlate_one" mnemonicParsing="false" text="plate#1">
            <toggleGroup>
               <ToggleGroup fx:id="getPlates"/>
            </toggleGroup>
         </RadioButton>
         <RadioButton fx:id="radioPlate_two" mnemonicParsing="false" text="plate#2" toggleGroup="$getPlates"/>
         <RadioButton fx:id="radioPlate_three" mnemonicParsing="false" text="plate#3" toggleGroup="$getPlates"/>
         <Button mnemonicParsing="false" text="Button"/>
      </children>
   </VBox>
</AnchorPane>
