<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="mainContainer"
            fx:id="anchorMain" prefHeight="480.0"
            prefWidth="720.0"
            stylesheets="@../../Styles/driverinfo.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.helpfox.main.Controller.User.DriverInfo"
>
   <children>
      <Label fx:id="name" alignment="BASELINE_LEFT" layoutX="24.0" layoutY="14.0">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="12.0" AnchorPane.rightAnchor="300" AnchorPane.topAnchor="80">
         <children>
            <RadioButton fx:id="radioPlate_one" mnemonicParsing="false" text="plate#1">
               <toggleGroup>
                  <ToggleGroup fx:id="getPlates" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="radioPlate_two" mnemonicParsing="false" toggleGroup="$getPlates" />
            <RadioButton fx:id="radioPlate_three" mnemonicParsing="false" toggleGroup="$getPlates" />
            <Button fx:id="btGatewayVehicle" mnemonicParsing="false" text="Button" />
         </children>
      </VBox>
   </children>
</AnchorPane>
