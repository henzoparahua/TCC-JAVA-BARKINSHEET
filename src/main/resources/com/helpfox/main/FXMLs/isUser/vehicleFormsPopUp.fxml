<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox id="mainContainer" alignment="TOP_CENTER" stylesheets="@../../Styles/forms_popup.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.helpfox.main.Controller.User.UserVehicleForms">
    <children>
        <VBox id="headerVbox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label id="headerLabel" alignment="CENTER_RIGHT" text="CADASTRAR VEICULOS">
                    <VBox.margin>
                        <Insets bottom="5.0" />
                    </VBox.margin>
                </Label>
                <Separator id="headerSeparator" prefWidth="200.0">
                    <VBox.margin>
                        <Insets top="5.0" />
                    </VBox.margin>
                </Separator>
            </children>
            <VBox.margin>
                <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
            </VBox.margin>
        </VBox>
        <VBox alignment="TOP_CENTER" spacing="18.0">
            <children>
                <Label styleClass="txtField" text="Dono do Veiculo" />
                <TextField promptText="Placa" styleClass="txtField" />
                <HBox alignment="CENTER" spacing="12.0">
                    <children>
                        <TextField id="hboxtxtField" promptText="Marca" styleClass="txtField" />
                        <TextField id="hboxtxtField" promptText="Cor" styleClass="txtField" />
                    </children>
                </HBox>
                <HBox alignment="TOP_CENTER" spacing="32.0">
                    <children>
                        <Button id="Button" fx:id="btConfirmVehicles" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-border-insets: 0; -fx-opaque-insets: 0;" text="CONFIRMAR" textAlignment="CENTER" />
                        <Button id="Button" fx:id="btCancelVehicles" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="CANCELAR" textAlignment="CENTER" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="38.0" top="8.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</VBox>
